{
  "name": "tamagui-translator",
  "description": "Translator with Transformers.js Tamagui Expo",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "expo run android",
    "check": "expo install --fix",
    "clean": "git clean -xdf",
    "doctor": "yarn dlx @yarnpkg/doctor && yarn dlx expo-doctor@latest",
    "ios": "expo run ios",
    "lint": "yarn dlx @biomejs/biome check --apply .",
    "start": "expo start -c",
    "update": "yarn upgrade-interactive",
    "vscode": "yarn dlx @yarnpkg/sdks vscode",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@tamagui/config": "latest",
    "@tamagui/constants": "latest",
    "@tamagui/font-inter": "latest",
    "@tamagui/linear-gradient": "latest",
    "@tamagui/lucide-icons": "latest",
    "@tamagui/metro-plugin": "latest",
    "@xenova/transformers": "^2.16.1",
    "expo": "^50.0.14",
    "expo-asset": "^9.0.2",
    "expo-file-system": "^16.0.8",
    "expo-font": "^11.10.3",
    "expo-linear-gradient": "^12.7.2",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.8",
    "expo-status-bar": "^1.11.1",
    "immer": "^10.0.4",
    "jotai": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "14.1.0",
    "react-native-web": "^0.19.10",
    "tamagui": "latest",
    "typescript": "^5.4.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@biomejs/biome": "^1.6.3",
    "@expo/metro-runtime": "^3.1.3",
    "@types/react": "^18.2.73",
    "babel-preset-expo": "^10.0.1",
    "metro": "^0.80.8",
    "vercel": "latest"
  },
  "packageManager": "yarn@4.1.1"
}
